<MyLayout>
	BoxLayout:
		orientation: 'vertical'
		size: (root.width, root.height)

		padding: 50
		spacing: 20

		Label:
			id:my_label
			text: 'Hello World'
			font_size: 32

		Button:
			text: 'Press Here'
			font_size: 32
			Image:
				source: 'images/kaneki.png'
				center_x: self.parent.center_x
				center_y: self.parent.center_y